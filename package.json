{
  "name": "syndesis-ui",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "generate": "node ./scripts/generate-ts.js",
    "copy:assets": "yarn ncp node_modules/syndesis.data.mapper/src/assets/dm src/assets/dm",
    "start": "yarn ng serve",
    "start:prod": "yarn ng serve -- --aot --prod",
    "lint": "yarn ng lint",
    "lint:fix": "yarn ng lint -- --fix",
    "test": "yarn ng test",
    "test:cc": "yarn ng test -- --watch=false --cc",
    "test:xvfb": "docker run --rm -v `pwd`:/syndesis-ui:z -w /syndesis-ui -u `id -u` docker.io/syndesis/karma-xvfb ./karma-xvfb.sh",
    "pree2e": "webdriver-manager update",
    "e2e": "better-npm-run e2e:local",
    "e2e:syndesis-qe": "better-npm-run e2e:syndesis-qe",
    "e2e:xvfb": "docker run -e SYNDESIS_UI_URL=$SYNDESIS_UI_URL --rm -v `pwd`:/syndesis-ui:z -w /syndesis-ui -u `id -u` docker.io/syndesis/karma-xvfb ./e2e-xvfb.sh",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js",
    "docker:build": "yarn ng build -- --aot --prod && docker build -t syndesis/syndesis-ui:latest ."
  },
  "betterScripts": {
    "e2e:local": {
      "command": "protractor",
      "env": {
        "TEST_ENV": "local"
      }
    },
    "e2e:syndesis-qe": {
      "command": "protractor",
      "env": {
        "TEST_ENV": "syndesis-qe"
      }
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "4.1.2",
    "@angular/compiler": "4.1.2",
    "@angular/core": "4.1.2",
    "@angular/forms": "4.1.2",
    "@angular/http": "4.1.2",
    "@angular/platform-browser": "4.1.2",
    "@angular/platform-browser-dynamic": "4.1.2",
    "@angular/router": "4.1.2",
    "@ng2-dynamic-forms/core": "1.4.9",
    "@ng2-dynamic-forms/ui-bootstrap": "1.4.9",
    "angular-oauth2-oidc-hybrid": "1.0.26",
    "angular2-toaster": "3.0.1",
    "better-npm-run": "^0.0.15",
    "bootstrap": "3.3.7",
    "chart.js": "^2.5.0",
    "core-js": "2.4.1",
    "file-saver": "^1.3.3",
    "jquery": "3.1.1",
    "lodash": "4.17.4",
    "merge": "^1.2.0",
    "ng2-charts": "1.5.0",
    "ngx-bootstrap": "1.6.6",
    "ngx-restangular": "1.0.9",
    "patternfly": "3.20.0",
    "pluralize": "3.1.0",
    "rxjs": "5.1.1",
    "syndesis.data.mapper": "https://github.com/syndesisio/syndesis-datamapper-ui.git#9476d91ec03c0e4885f3005876c67216130ef98d",
    "tether": "1.4.0",
    "ts-helpers": "1.1.2",
    "typescript-logging": "^0.2.1",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.3",
    "@angular/compiler-cli": "4.1.2",
    "@types/chai-as-promised": "^0.0.29",
    "@types/cucumber": "^0.0.37",
    "@types/file-saver": "0.0.0",
    "@types/jasmine": "2.5.43",
    "@types/jquery": "^2.0.41",
    "@types/lodash": "4.14.52",
    "@types/node": "7.0.5",
    "@types/pluralize": "0.0.27",
    "@types/protractor": "^4.0.0",
    "awesome-typescript-loader": "3.0.4",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "codelyzer": "2.0.0",
    "cucumber": "^1.3.1",
    "cucumber-html-reporter": "^0.3.7",
    "cucumber-tsflow": "^1.1.2",
    "fs-extra": "^2.0.0",
    "jasmine-core": "2.5.2",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "0.2.3",
    "karma-jasmine": "1.1.0",
    "karma-junit-reporter": "1.2.0",
    "ncp": "^2.0.0",
    "protractor": "5.1.1",
    "protractor-cucumber-framework": "^0.6.0",
    "sass-lint": "1.10.2",
    "sw-precache": "5.0.0",
    "swagger-js-codegen": "1.7.12",
    "ts-node": "2.1.0",
    "tslint": "^4.5.1",
    "typescript": "2.1.6",
    "webdriver-manager": "12.0.2"
  }
}
