{
  "name": "syndesis-ui",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.json -s",
    "format": "yarn prettier -- --single-quote --trailing-comma all --write \"./src/**/*.{ts,css,scss,js,json}\"",
    "generate": "node ./scripts/generate-ts.js",
    "copy:assets": "yarn ncp node_modules/syndesis.data.mapper/src/assets/dm src/assets/dm",
    "start": "yarn ng serve -- --ssl true --ssl-key localhost.key --ssl-cert localhost.crt --proxy-config proxy.conf.js",
    "start:prod": "yarn ng serve -- --ssl true --ssl-key localhost.key --ssl-cert localhost.crt --proxy-config proxy.conf.js --aot --prod",
    "lint": "yarn ng lint",
    "lint:fix": "yarn ng lint -- --fix",
    "test": "yarn ng test",
    "test:cc": "yarn ng test -- --watch=false --cc",
    "test:xvfb": "docker run --rm -v `pwd`:/syndesis-ui:z -w /syndesis-ui -u `id -u` docker.io/syndesis/karma-xvfb ./karma-xvfb.sh",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js",
    "docker:build": "yarn ng build -- --aot --prod && docker build -f docker/Dockerfile -t syndesis/syndesis-ui:latest . && docker tag syndesis/syndesis-ui:latest docker.io/syndesis/syndesis-ui:latest",
    "commit": "yarn git-cz"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.1.2",
    "@angular/common": "4.1.2",
    "@angular/compiler": "4.1.2",
    "@angular/core": "4.1.2",
    "@angular/forms": "4.1.2",
    "@angular/http": "4.1.2",
    "@angular/platform-browser": "4.1.2",
    "@angular/platform-browser-dynamic": "4.1.2",
    "@angular/router": "4.1.2",
    "@ng2-dynamic-forms/core": "1.4.20",
    "@ng2-dynamic-forms/ui-bootstrap": "1.4.20",
    "angular2-text-mask": "^8.0.1",
    "better-npm-run": "^0.0.15",
    "bootstrap": "3.3.7",
    "chart.js": "^2.5.0",
    "core-js": "2.4.1",
    "file-saver": "^1.3.3",
    "jquery": "3.1.1",
    "merge": "^1.2.0",
    "ng2-charts": "1.6.0",
    "ngx-bootstrap": "1.9.1",
    "ngx-chips": "^1.4.5",
    "ngx-restangular": "1.0.9",
    "patternfly": "3.26.1",
    "patternfly-ng": "^0.9.2",
    "pluralize": "5.0.0",
    "rxjs": "5.4.1",
    "syndesis.data.mapper": "https://github.com/atlasmap/atlasmap-ui.git#d5dcb718fe12f754749a54f2eb85b925a41cc9b0",
    "tether": "1.4.0",
    "ts-helpers": "1.1.2",
    "typescript-logging": "^0.2.1",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.3",
    "@angular/compiler-cli": "4.1.2",
    "@compodoc/compodoc": "^1.0.0-beta.14",
    "@types/chai-as-promised": "^0.0.29",
    "@types/cucumber": "^0.0.37",
    "@types/file-saver": "0.0.0",
    "@types/jasmine": "2.5.43",
    "@types/jquery": "^2.0.41",
    "@types/node": "7.0.5",
    "@types/pluralize": "0.0.27",
    "awesome-typescript-loader": "3.1.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "codelyzer": "^2.1.1",
    "commitizen": "^2.9.6",
    "cucumber": "^1.3.1",
    "cucumber-html-reporter": "^0.3.7",
    "cucumber-tsflow": "^1.1.2",
    "cz-conventional-changelog": "^2.0.0",
    "fs-extra": "^2.0.0",
    "jasmine-core": "2.5.2",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "0.2.3",
    "karma-jasmine": "1.1.0",
    "karma-junit-reporter": "1.2.0",
    "karma-mocha-reporter": "2.2.3",
    "ncp": "^2.0.0",
    "patternfly-sass": "3.23.2",
    "prettier": "^1.5.2",
    "sass-lint": "1.10.2",
    "sw-precache": "5.0.0",
    "swagger-js-codegen": "1.7.12",
    "ts-node": "^3.0.2",
    "tslint": "^4.5.0",
    "typescript": "^2.3.2",
    "webdriver-manager": "12.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
