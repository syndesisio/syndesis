{
  "name": "syndesis-ui",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "description": "Presentation layer for Syndesis - a flexible, customizable, cloud-hosted platform that provides core integration capabilities as a service.",
  "homepage": "https://github.com/syndesisio/syndesis-ui",
  "bugs": "https://github.com/syndesisio/syndesis-ui/issues",
  "scripts": {
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.json -s",
    "copy:assets": "yarn ncp node_modules/@atlasmap/atlasmap.data.mapper/src/assets/dm src/assets/dm",
    "format": "yarn prettier --no-color --single-quote --trailing-comma all --write \"./src/**/*.{ts,css,scss,js,json}\"",
    "generate": "node ./scripts/generate-ts.js",
    "start": "yarn ng serve --ssl true --ssl-key localhost.key --ssl-cert localhost.crt --proxy-config proxy.conf.js",
    "start:prod": "yarn ng serve --ssl true --ssl-key localhost.key --ssl-cert localhost.crt --proxy-config proxy.conf.js --aot --prod",
    "start:minishift": "./scripts/minishift-setup.sh && yarn ng serve --host 0.0.0.0 --disable-host-check --public-host $(oc get routes syndesis --template '{{.spec.host}}')",
    "build:ci": "yarn ng build --aot --prod --progress=false",
    "lint": "yarn ng lint --format stylish",
    "lint:fix": "yarn ng lint --fix",
    "test": "yarn ng test --sourcemaps=false",
    "test:cc": "yarn ng test --watch=false --cc",
    "test:ci": "yarn ng test --watch=false --progress=false",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js",
    "docker:build": "yarn ng build --aot --prod && docker build -f docker/Dockerfile -t syndesis/syndesis-ui:latest . && docker tag syndesis/syndesis-ui:latest docker.io/syndesis/syndesis-ui:latest"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@atlasmap/atlasmap.data.mapper": "1.32.0-SNAPSHOT.20171018190510",
    "@ng-dynamic-forms/core": "5.4.1",
    "@ng-dynamic-forms/ui-bootstrap": "5.4.1",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "@ngrx/store-devtools": "^4.1.1",
    "angular2-text-mask": "^8.0.1",
    "better-npm-run": "^0.0.15",
    "bootstrap": "3.3.7",
    "chart.js": "^2.5.0",
    "core-js": "2.4.1",
    "file-saver": "^1.3.3",
    "jquery": "3.1.1",
    "merge": "^1.2.0",
    "ng2-charts": "1.6.0",
    "ng2-file-upload": "1.3.0",
    "ngx-bootstrap": "2.0.0-rc.0",
    "ngx-chips": "1.6.2",
    "ngx-restangular": "1.0.9",
    "ngx-tour-ngx-bootstrap": "^1.0.4",
    "patternfly": "3.26.1",
    "patternfly-ng": "^0.13.3",
    "pluralize": "5.0.0",
    "rxjs": "^5.5.0",
    "tether": "1.4.0",
    "ts-helpers": "1.1.2",
    "typescript-logging": "0.4.0",
    "url": "^0.11.0",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.2.8",
    "@angular/compiler-cli": "^5.0.0",
    "@compodoc/compodoc": "1.0.0-beta.11",
    "@ngtools/webpack": "1.5.6",
    "@types/chai-as-promised": "^0.0.29",
    "@types/cucumber": "^0.0.37",
    "@types/file-saver": "0.0.0",
    "@types/jasmine": "2.5.43",
    "@types/jquery": "^2.0.41",
    "@types/node": "7.0.5",
    "@types/pluralize": "0.0.27",
    "awesome-typescript-loader": "3.1.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "codelyzer": "~4.0.1",
    "cucumber": "^1.3.1",
    "cucumber-html-reporter": "^0.3.7",
    "cucumber-tsflow": "^1.1.2",
    "fs-extra": "^2.0.0",
    "jasmine-core": "2.5.2",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "0.2.3",
    "karma-jasmine": "1.1.0",
    "karma-junit-reporter": "1.2.0",
    "karma-mocha-reporter": "2.2.3",
    "my-local-ip": "^1.0.0",
    "ncp": "^2.0.0",
    "patternfly-sass": "3.23.2",
    "prettier": "^1.5.2",
    "puppeteer": "^0.12.0",
    "sass-lint": "1.10.2",
    "sw-precache": "5.0.0",
    "swagger-js-codegen": "1.7.12",
    "ts-node": "^3.0.2",
    "tslint": "^5.0.0",
    "typescript": "~2.4.0",
    "webdriver-manager": "12.0.2"
  }
}
