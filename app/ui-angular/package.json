    "awesome-typescript-loader": "3.1.3",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "codelyzer": "4.0.2",
    "fs-extra": "4.0.3",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "karma": "3.1.4",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.4.3",
    "karma-jasmine": "1.1.2",
    "karma-junit-reporter": "1.2.0",
    "karma-mocha-reporter": "2.2.3",
    "my-local-ip": "1.0.0",
    "ncp": "2.0.0",
    "patch-package": "5.1.1",
    "patternfly-sass": "3.23.2",
    "prettier": "1.15.3",
    "puppeteer": "^1.13.0",
    "rimraf": "2.6.2",
    "rxjs-compat": "6.3.3",
    "rxjs-tslint": "0.1.6",
