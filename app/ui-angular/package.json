  },
  "resolutions": {
    "bootstrap": "~3.4",
    "bootstrap-select": "1.13.6",
    "codemirror": "5.42.2",
    "extend": "3.0.2",
    "fstream": "1.0.12",
    "jquery": "3.4.1",
    "lodash": "4.17.11",
    "ngx-bootstrap": "3.1.4"
  },
  "snyk": true
}
