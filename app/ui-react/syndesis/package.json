    "@types/react-helmet": "^6.1.0",
    "@types/react-loadable": "*",
    "@types/react-router-dom": "^5.1.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^9.1.0",
    "cpr": "^3.0.1",
    "cypress": "4.0.2",
    "my-local-ip": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^2.1.1",
    "react-scripts": "3.4.1",
    "talkback": "^1.10.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-react": "^4.0.0",
    "webpack": "^4.41.5",
    "typescript": "~3.9.7",
    "yargs": "^15.1.0"
